{
    "name": "lysice/laravel-sso",
    "description": "Simple PHP SSO integration for Laravel",
    "keywords": ["laravel", "sso", "authentication", "login"],
    "homepage": "https://github.com/Lysice/laravel-sso",
    "license": "MIT",
    "authors": [
      {
        "name": "Lysice",
        "email": "snowist@126.com",
        "homepage": "https://github.com/Lysice",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": ">=6.3",
        "laravel/framework": ">=5.5",
        "zefy/php-simple-sso": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "Lysice\\LaravelSSO\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "Lysice\\LaravelSSO\\SSOServiceProvider"
        ]
      }
    }
}
